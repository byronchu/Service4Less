<p id="notice"><%= notice %></p>

<h1>Listing Service Dates</h1>

<table>
  <thead>
    <tr>
      
      <th>Date</th>
      <th>Qty avail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_dates.each do |service_date| %>
      <tr>
        <td><%= service_date.date %></td>
        <td><%= service_date.qty_avail %>  </td>
        <td><%= link_to 'Show', service_date %></td>
    
        <td><%= link_to 'Edit', edit_service_date_path(service_date) %></td>
        <td><%= link_to 'Destroy', service_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service date', new_service_date_path %>
