<%= simple_form_for(@service) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.association :user %>
    <%= f.association :category %>
    <%= f.input :description %>
    <%#= f.text_field :country %>
    <%= f.input :state %>
    <%= f.input :city %>
    <%= f.input :price %>
    <%#= f.association :service_dates %>
    <%= f.input :pic, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
