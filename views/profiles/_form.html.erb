<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user %>
    <%= f.input :name %>
    <%= f.input :phone %>
    <%= f.input :country %>
    <%= f.input :state %>
    <%= f.input :city %>
    <%= f.input :service_provider %>
    <%= f.input :provider_name %>
    <%= f.input :provider_logo_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
